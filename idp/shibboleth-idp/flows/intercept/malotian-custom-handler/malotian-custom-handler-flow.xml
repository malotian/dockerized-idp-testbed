<?xml version="1.0" encoding="UTF-8"?>

<flow xmlns="http://www.springframework.org/schema/webflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow.xsd" parent="intercept.abstract">

	<!-- This is a login flow for external authentication handled outside the webflow engine. -->

	<action-state id="GluuOutboundMessageProcessorState">
		<evaluate expression="GluuOutboundMessageProcessor" />
		<evaluate expression="'proceed'" />
		<transition on="proceed" to="proceed" />
	</action-state>

	<bean-import resource="malotian-custom-handler-beans.xml" />

</flow>

